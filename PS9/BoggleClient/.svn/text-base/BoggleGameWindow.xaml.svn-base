<Window x:Name="GameWindow" x:Class="BoggleClient.BoggleGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Boggle" Height="360" Width="715" MaxHeight="360" MaxWidth="715">
    <Grid Background="#FF8F8F8F" Margin="0,0,-1,-1">
        <Grid Margin="30,47,170,10">
            <Button x:Name="bb_button_1" Content="?" HorizontalAlignment="Left" Height="58" VerticalAlignment="Top" Width="57" Margin="0,-31,0,0" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_5" Content="?" HorizontalAlignment="Left" Height="57" Margin="0,33,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_9" Content="?" HorizontalAlignment="Left" Height="57" Margin="0,98,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_13" Content="?" HorizontalAlignment="Left" Height="58" Margin="0,161,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_2" Content="?" HorizontalAlignment="Left" Height="58" Margin="64,-31,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_3" Content="?" HorizontalAlignment="Left" Height="58" Margin="129,-31,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_4" Content="?" HorizontalAlignment="Left" Height="58" Margin="193,-31,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_6" Content="?" HorizontalAlignment="Left" Height="57" Margin="64,33,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_7" Content="?" HorizontalAlignment="Left" Height="57" Margin="129,33,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_8" Content="?" HorizontalAlignment="Left" Height="57" Margin="193,33,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_10" Content="?" HorizontalAlignment="Left" Height="57" Margin="64,98,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_11" Content="?" HorizontalAlignment="Left" Height="57" Margin="129,98,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_12" Content="?" HorizontalAlignment="Left" Height="57" Margin="193,98,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_14" Content="?" HorizontalAlignment="Left" Height="58" Margin="64,161,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_15" Content="?" HorizontalAlignment="Left" Height="58" Margin="129,161,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="bb_button_16" Content="?" HorizontalAlignment="Left" Height="58" Margin="193,161,0,0" VerticalAlignment="Top" Width="57" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="20"/>
            <Button x:Name="enter_word_button" Content="ENTER" HorizontalAlignment="Left" Height="29" Margin="50,233,0,0" VerticalAlignment="Top" Width="71" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="14">
                <Button.Background>
                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                        <GradientStop Color="#FFF3F3F3" Offset="0"/>
                        <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                        <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                        <GradientStop Color="#FF5FBB4A" Offset="1"/>
                    </LinearGradientBrush>
                </Button.Background>
            </Button>
            <Button x:Name="clear_word_button" Content="CLEAR" HorizontalAlignment="Left" Height="29" Margin="129,233,0,0" VerticalAlignment="Top" Width="71" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="14">
                <Button.Background>
                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                        <GradientStop Color="#FFF3F3F3" Offset="0"/>
                        <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                        <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                        <GradientStop Color="#FFBB694A" Offset="1"/>
                    </LinearGradientBrush>
                </Button.Background>
            </Button>
            <TextBlock x:Name="entered_word_label" HorizontalAlignment="Left" Height="50" Margin="302,212,-119.8,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="301" TextAlignment="Center" FontFamily="/BoggleClient;component/Resources/#standard 09_56" FontSize="24" FontWeight="Bold"/>
        </Grid>
        <Grid Margin="334,19,37.2,60.2">
            <Rectangle HorizontalAlignment="Left" Height="236" Stroke="#FFDBDCDC" VerticalAlignment="Top" Width="310" StrokeThickness="10" StrokeEndLineCap="Round" StrokeStartLineCap="Square" StrokeLineJoin="Bevel" Margin="-6,-5,0,0" Fill="Black">
                <Rectangle.Effect>
                    <DropShadowEffect/>
                </Rectangle.Effect>
            </Rectangle>
            <Rectangle x:Name="time_bg" HorizontalAlignment="Left" Height="78" Margin="90,48,0,0" Stroke="White" VerticalAlignment="Top" Width="118" StrokeLineJoin="Bevel" RadiusY="11.263" RadiusX="11.263" StrokeDashArray="5" StrokeEndLineCap="Square"/>
            <Rectangle x:Name="p1_score_bg" HorizontalAlignment="Left" Height="57" Margin="9,113,0,0" Stroke="White" VerticalAlignment="Top" Width="75" StrokeLineJoin="Bevel" RadiusY="7" RadiusX="7" StrokeDashArray="5"/>
            <Label x:Name="scoreboard_label" Content="SCOREBOARD" HorizontalAlignment="Left" Height="40" Margin="54,8,0,0" VerticalAlignment="Top" Width="194" FontFamily="/BoggleClient;component/Resources/#standard 09_56" FontSize="18" Background="{x:Null}" Foreground="#FFF7F3F3"/>
            <Button x:Name="quit_button" Content="QUIT" HorizontalAlignment="Left" Height="28" Margin="110,185,0,0" VerticalAlignment="Top" Width="74" FontFamily="/BoggleClient;component/Resources/#header 08_65" FontSize="14" Click="quit_button_Click"/>
            <Rectangle x:Name="p1_score_bg_Copy" HorizontalAlignment="Left" Height="57" Margin="215,113,0,0" Stroke="White" VerticalAlignment="Top" Width="75" StrokeLineJoin="Bevel" RadiusY="7" RadiusX="7" StrokeDashOffset="5" StrokeDashCap="Square" StrokeDashArray="5"/>
            <TextBlock x:Name="p2_name_label" HorizontalAlignment="Left" Height="30" Margin="215,83,0,0" TextWrapping="Wrap" Text="ENEMY" VerticalAlignment="Top" Width="76" Foreground="White" FontFamily="/BoggleClient;component/Resources/#standard 09_56" TextAlignment="Center"/>
            <TextBlock x:Name="p1_name_label" HorizontalAlignment="Left" Height="30" Margin="9,83,0,0" TextWrapping="Wrap" Text="YOU" VerticalAlignment="Top" Width="76" Foreground="White" FontFamily="/BoggleClient;component/Resources/#standard 09_56" TextAlignment="Center"/>
            <TextBlock x:Name="p1_name_label_Copy" HorizontalAlignment="Left" Height="60" Margin="91,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="113" Foreground="White" FontFamily="/BoggleClient;component/Resources/#standard 09_56" TextAlignment="Center" FontSize="13"><Run Text="TIME"/><LineBreak/><Run Text="REMAINING"/></TextBlock>
            <TextBlock x:Name="p2_score_count_label" HorizontalAlignment="Left" Height="43" Margin="219,116,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="72" Foreground="White" FontFamily="/BoggleClient;component/Resources/#standard 09_56" FontSize="24" TextAlignment="Center" RenderTransformOrigin="0.447,0.301"/>
            <TextBlock x:Name="p1_score_count_label" HorizontalAlignment="Left" Height="45" Margin="13,116,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="72" Foreground="White" FontFamily="/BoggleClient;component/Resources/#standard 09_56" FontSize="24" TextAlignment="Center"/>
            <TextBlock x:Name="time_count_label" HorizontalAlignment="Left" Height="65" TextWrapping="Wrap" Text="000" VerticalAlignment="Top" Width="117" Foreground="White" FontFamily="/BoggleClient;component/Resources/#standard 09_56" FontSize="35" TextAlignment="Center" RenderTransformOrigin="0.447,0.301" Margin="95,51,0,0"/>
        </Grid>
    </Grid>
</Window>
